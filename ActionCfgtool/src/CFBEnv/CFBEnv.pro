#include ($$(PWD)../plate.pri)

TEMPLATE = lib
QT      += core xml network
DEFINES += CFBENV_LIB BUILDING_CURLPP

INCLUDEPATH += $$(PWD)../common \
               $${TRD}/curlpp/include \
               $${TRD}/curl/include \
               $${SRC}/cfbbase/common \
               $${SRC}/cfbbase/utils \
               $${SRC}/cfbbase/cfbhttpclient \
               $${SRC}/cfbbase/qutils

win32{
    CONFIG(release, debug|release){
        TARGET  = CFBEnv
        DESTDIR = $${BIN}/$${PLATE_NAME}/$${DIR_NAME}
        LIBS += -L$${DESTDIR} \
                -lutils \
                -lqutils \
                -lcfbhttpclient \
                -L$${TRD}/curl/lib/$${PLATE_NAME}/$${DIR_NAME} \
                -llibcurl \
                -L$${TRD}/curlpp/lib/$${PLATE_NAME}/$${DIR_NAME} \
                -lcurlpp
    }
    CONFIG(debug, debug|release){
        TARGET   = CFBEnvd
        DESTDIR = $${BIN}/$${PLATE_NAME}/$${DIR_NAME}
        LIBS += -L$${DESTDIR} \
                -lutilsd \
                -lqutilsd \
                -lcfbhttpclientd \
                -L$${TRD}/curl/lib/$${PLATE_NAME}/$${DIR_NAME} \
                -llibcurld \
                -L$${TRD}/curlpp/lib/$${PLATE_NAME}/$${DIR_NAME} \
                -lcurlppd
    }
}

HEADERS += updaterinterface.h \
           configs.h \
           iniconfig.h \
           stdafx.h \
           cfbenv_global.h \
           qclient.h
SOURCES += qclient.cpp \
           configs.cpp \
           iniconfig.cpp \
           stdafx.cpp \
           updaterinterface.cpp

